        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="com.fundamental/images/slider_image/slider_image_01.jpg" alt="" style="width:100%;">
            </div>

            <div class="item">
                <img src="com.fundamental/images/slider_image/slider_image_02.jpg" alt="" style="width:100%;">
            </div>

            <div class="item">
                <img src="com.fundamental/images/slider_image/slider_image_03.jpg" alt="" style="width:100%;">
            </div>

            <div class="item">
                <img src="com.fundamental/images/slider_image/slider_image_04.jpg" alt="" style="width:100%;">

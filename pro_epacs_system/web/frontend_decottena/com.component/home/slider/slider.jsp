            <div class="item">
                <img src="com.fundamental/images/slider_image/slider_image_05.jpg" alt="" style="width:100%;">
            </div>

            <div class="item">
                <img src="com.fundamental/images/slider_image/slider_image_06.jpg" alt="" style="width:100%;">
            </div>

            <div class="item">
                <img src="com.fundamental/images/slider_image/slider_image_07.jpg" alt="" style="width:100%;">
            </div>

            <div class="item">
                <img src="com.fundamental/images/slider_image/slider_image_08.jpg" alt="" style="width:100%;">
            </div>

            <div class="item">
                <img src="com.fundamental/images/slider_image/slider_image_09.jpg" alt="" style="width:100%;">
            </div>
        </div>

        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
